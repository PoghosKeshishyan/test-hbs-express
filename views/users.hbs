
<h1>{{ title }}</h1>

<table border="1" cellpadding="10" cellspacing="0">
    <thead>
        <tr>
            <th>N</th>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{ inc @index }}</td>
            <td>{{ this.name }}</td>
            <td>{{ this.email }}</td>
            <td>{{ this.age }}</td>
            <td>
                <a href="/users/{{ this.id }}">
                    go
                </a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
